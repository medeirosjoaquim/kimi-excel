{
  "name": "@kimi-excel/client",
  "version": "1.0.0",
  "private": true,
  "description": "React frontend for kimi-excel",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
