{
  "name": "kimi-excel",
  "version": "1.0.0",
  "private": true,
  "description": "Excel analysis tool using Kimi (Moonshot AI) with server/client architecture",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @kimi-excel/server --filter @kimi-excel/client --parallel dev",
    "dev:server": "pnpm --filter @kimi-excel/server dev",
    "dev:client": "pnpm --filter @kimi-excel/client dev",
    "dev:cli": "pnpm --filter @kimi-excel/cli dev",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck"
  },
  "keywords": [
    "kimi",
    "moonshot",
    "excel",
    "csv",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
